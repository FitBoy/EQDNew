#import "GLFanKuiViewController.h"
#import "TestModel.h"
#import "FBThree_noimg122TableViewCell.h"
@interface GLFanKuiViewController ()<UITableViewDelegate,UITableViewDataSource>
{
    UITableView *tableV;
    NSMutableArray *arr_jilu;
}

@end

@implementation GLFanKuiViewController
-(void)viewWillAppear:(BOOL)animated
{
    [super viewWillAppear:animated];
    NSArray *arr = @[@{
                         @"name":@"姓名1",
                         @"title":@"标题",
                         @"date":@"2017-03-23"
                         },@{
                         @"name":@"姓名2",
                         @"title":@"标题2",
                         @"date":@"2017-03-24"
                         }];
    for (NSDictionary *dic2 in arr) {
        TestModel *model = [TestModel mj_objectWithKeyValues:dic2];
        [arr_jilu addObject:model];
    }
    [tableV reloadData];

}
- (void)viewDidLoad {
    [super viewDidLoad];
    self.navigationItem.title =@"反馈记录";
    arr_jilu =[NSMutableArray arrayWithCapacity:0];
    tableV = [[UITableView alloc]initWithFrame:CGRectMake(0, 0, DEVICE_WIDTH, DEVICE_HEIGHT) style:UITableViewStylePlain];
    tableV.delegate=self;
    tableV.dataSource=self;
    [self.view addSubview:tableV];
    tableV.rowHeight=60;
    
    
}

    

#pragma  mark - 表的数据源
-(NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section
{
    return arr_jilu.count;
}
-(UITableViewCell*)tableView:(UITableView *)tableView cellForRowAtIndexPath:(NSIndexPath *)indexPath{
    static NSString *cellId=@"cellID";
    FBThree_noimg122TableViewCell *cell =[tableView dequeueReusableCellWithIdentifier:cellId];
    if (!cell) {
        cell = [[FBThree_noimg122TableViewCell alloc]initWithStyle:UITableViewCellStyleDefault reuseIdentifier:cellId];
        cell.accessoryType = UITableViewCellAccessoryNone;
    }
    TestModel *model =arr_jilu[indexPath.row];
    [cell setModel:model];
    
    return cell;
}

#pragma  mark - 表的协议代理
-(void)tableView:(UITableView *)tableView didSelectRowAtIndexPath:(NSIndexPath *)indexPath
{
    
}



@end
