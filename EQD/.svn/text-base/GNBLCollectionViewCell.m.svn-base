#import "GNBLCollectionViewCell.h"

#import <Masonry.h>
@implementation GNBLCollectionViewCell
-(void)setbiaoji:(NSString*)biji  Withmodel:(GNmodel*)model;
{
    self.IV_img = [[UIImageView alloc]init];
    [self addSubview:self.IV_img];
    self.L_title = [[UILabel alloc]init];
    self.L_title.font =[UIFont systemFontOfSize:12];
    self.L_title.textAlignment =NSTextAlignmentCenter;
    [self addSubview:self.L_title];
    
            self.IV_img.image=[UIImage imageNamed:model.img];
            self.L_title.text =model.name;

    
    self.biaoji = biji;
    
    //下面增加约束
    [self.IV_img mas_makeConstraints:^(MASConstraintMaker *make) {
        make.size.mas_equalTo(CGSizeMake(50, 50));
        make.centerX.mas_equalTo(self.mas_centerX);
        make.top.mas_equalTo(self.mas_top);
    }];
    
    [self.L_title mas_makeConstraints:^(MASConstraintMaker *make) {
         make.centerX.mas_equalTo(self.mas_centerX);
        make.bottom.mas_equalTo(self.mas_bottom);
        make.size.mas_equalTo(CGSizeMake(50, 15));
        
    }];
    
    
    
    
    
}
@end
